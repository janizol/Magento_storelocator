{
  "name":"janizol/magento-storelocator",
  "type":"magento-module",
  "homepage":"https://github.com/janizol/Magento_storelocator/",
  "description":"Magento extension for a storelocator.",
  "authors":[
    {
      "name":"Janizol Adams",
      "email":"janizoladams@gmail.com"
    }
  ],
  "require":{
    "magento-hackathon/magento-composer-installer":"*"
  },
  "extra":{
    "map":[
      ["app/etc/modules/Pepkor_Storelocator.xml", "app/etc/modules/Pepkor_Storelocator.xml"],
      ["app/code/local/Pepkor/Storelocator/", "app/code/local/Pepkor/Storelocator/"],
      ["app/design/adminhtml/default/default/layout/", "app/design/adminhtml/default/default/layout/"],
      ["app/design/adminhtml/default/default/template/storelocator/", "app/design/adminhtml/default/default/template/storelocator/"],
      ["app/design/frontend/base/default/layout/", "app/design/frontend/base/default/layout/"],
      ["app/design/frontend/base/default/template/storelocator/", "app/design/frontend/base/default/template/storelocator/"]
    ]
  }
}